#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5dd65c9304f0 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x5dd65c97d9c0_0 .var "clk", 0 0;
v0x5dd65c97da60_0 .var "reset", 0 0;
S_0x5dd65c930680 .scope module, "streamer" "riscv_command_streamer" 2 6, 3 1 0, S_0x5dd65c9304f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
P_0x5dd65c930860 .param/l "MEM_SIZE" 0 3 1, +C4<00000000000000000000010000000000>;
v0x5dd65c96a060_0 .net "clk", 0 0, v0x5dd65c97d9c0_0;  1 drivers
v0x5dd65c97d5c0_0 .var "instruction", 31 0;
v0x5dd65c97d6a0 .array "memory", 1023 0, 31 0;
v0x5dd65c97d770_0 .var "pc", 31 0;
v0x5dd65c97d850_0 .net "reset", 0 0, v0x5dd65c97da60_0;  1 drivers
E_0x5dd65c96b3b0 .event posedge, v0x5dd65c97d850_0, v0x5dd65c96a060_0;
    .scope S_0x5dd65c930680;
T_0 ;
    %vpi_call 3 17 "$readmemh", "program.hex", v0x5dd65c97d6a0 {0 0 0};
    %pushi/vec4 1024, 0, 32;
    %store/vec4 v0x5dd65c97d770_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x5dd65c930680;
T_1 ;
    %wait E_0x5dd65c96b3b0;
    %load/vec4 v0x5dd65c97d850_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 1024, 0, 32;
    %assign/vec4 v0x5dd65c97d770_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5dd65c97d770_0;
    %ix/load 5, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 5;
    %ix/vec4 4;
    %load/vec4a v0x5dd65c97d6a0, 4;
    %store/vec4 v0x5dd65c97d5c0_0, 0, 32;
    %load/vec4 v0x5dd65c97d770_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %vpi_call 3 31 "$display", "PC: %h, Instruction: %h, word in processor %h", v0x5dd65c97d770_0, v0x5dd65c97d5c0_0, S<0,vec4,u32> {1 0 0};
    %load/vec4 v0x5dd65c97d770_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x5dd65c97d770_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5dd65c9304f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5dd65c97d9c0_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x5dd65c97d9c0_0;
    %inv;
    %store/vec4 v0x5dd65c97d9c0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5dd65c9304f0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5dd65c97da60_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5dd65c97da60_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "riscv_command_streamer_tb.v";
    "riscv_command_streamer.v";
